      "columna3,",
      "columna4,",
      "columna5,",
      "columna6",
      "from #tabla",
      "where columna1 = @i_columna1",
      "and   columna2 = @i_columna2",
      "and   columna3 = @i_columna3",
      "and   columna4 = @i_columna4",
